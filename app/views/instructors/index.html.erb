
<h1>Listing Instructors</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @instructors.each do |instructor| %>
      <tr>
        <td><%=instructor.first + " " + instructor.last%></td>
        <td><%=instructor.email%></td>
        <td><%=instructor.comment%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
